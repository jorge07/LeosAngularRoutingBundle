{
    "name": "leos/angular-ui-routing-bundle",
    "homepage": "https://github.com/jorge07/LeosAngularRoutingBundle",
    "description": "Adapting Angular UI-ROUTER to use it with Symfony2",
    "version": "0.1.0",
    "keywords": [
        "symfony2",
        "symfony",
        "symfony 2",
        "angular",
        "angularjs",
        "angular ui",
        "ui-router"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Arco",
            "homepage": "https://github.com/jorge07",
            "email": "jorge.arcoma@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "symfony/symfony": "2.*"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x"
        }
    },
    "autoload": {
        "psr-0": { "Leos\\AngularRoutingBundle": "" }
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    }
}
